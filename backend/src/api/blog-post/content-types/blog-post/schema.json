{
  "kind": "collectionType",
  "collectionName": "blog_posts",
  "info": {
    "singularName": "blog-post",
    "pluralName": "blog-posts",
    "displayName": "Blog post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "blocks",
      "required": true
    },
    "date": {
      "type": "date"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "blog_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::blog-category.blog-category",
      "inversedBy": "blog_posts"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "seo_section": {
      "type": "component",
      "repeatable": false,
      "component": "seo.seo-section"
    }
  }
}
